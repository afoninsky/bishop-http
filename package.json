{
  "name": "@fulldive/bishop-http",
  "version": "3.0.0",
  "description": "http-transport plugin for bishop.js library",
  "main": "index.js",
  "scripts": {
    "test": "eslint src && nyc ava && nyc report --reporter=html"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=7.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/afoninsky/bishop-http.git"
  },
  "author": "Andrey Afoninsky",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/afoninsky/bishop-http/issues"
  },
  "homepage": "https://github.com/afoninsky/bishop-http#readme",
  "devDependencies": {
    "@fulldive/bishop": "^1.0.0",
    "ava": "^0.25.0",
    "eslint": "^4.18.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "nyc": "^11.0.2",
    "prettier": "^1.11.1"
  },
  "dependencies": {
    "@fulldive/common": "^2.4.0",
    "koa": "^2.2.0",
    "koa-bodyparser": "^4.2.0",
    "koa-router": "^7.4.0",
    "lodash": "^4.17.4",
    "request": "^2.81.0",
    "request-promise": "^4.2.1"
  },
  "ava": {
    "verbose": true,
    "source": [
      "**/*js"
    ]
  },
  "nyc": {
    "include": [
      "src/**.js"
    ]
  }
}
