{
  "name": "bishop-http",
  "version": "1.0.0",
  "description": "http-transport plugin for bishop.js library",
  "main": "index.js",
  "scripts": {
    "test": "eslint src && nyc ava && nyc report --reporter=html"
  },
  "engines": {
    "node": ">=7.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/afoninsky/bishop-http.git"
  },
  "author": "Andrey Afoninsky",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/afoninsky/bishop-http/issues"
  },
  "homepage": "https://github.com/afoninsky/bishop-http#readme",
  "devDependencies": {
    "ava": "^0.18.2",
    "bishop": "^4.1.0",
    "eslint": "^3.16.1",
    "eslint-plugin-output-todo-comments": "0.0.7",
    "nyc": "^10.1.2"
  },
  "dependencies": {
    "koa": "next",
    "koa-bodyparser": "next",
    "koa-router": "next",
    "lodash": "^4.17.4",
    "request": "^2.79.0",
    "request-promise": "^4.1.1"
  },
  "ava": {
    "verbose": true,
    "source": [
      "**/*js"
    ]
  },
  "nyc": {
    "include": [
      "src/**.js"
    ]
  }
}
